pipeline{
    agent any
    stages {
        stage ('stage1') {
            steps {
                sh 'sleep 10'
                echo "this is stage 1 of pipeline"

            }
        }
        stage ('stage 2') {
            steps {
                sh 'sleep 10'
                echo "this is stage 2 of pipeline"
            }
        }
        stage ('stage 3') {
            steps {
                sh 'sleep 10'
                echo "this is stage 3 of pipeline"
            }    
        }


    }


}
